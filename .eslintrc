{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "no-console": "off",
    "quotes": ["off", "double"],
    "semi": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "capitalized-comments":"error"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  }

}